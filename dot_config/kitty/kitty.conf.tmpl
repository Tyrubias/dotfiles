font_family             Iosevka Term
bold_font               Iosevka Term Bold
italic_font             Iosevka Term Italic
bold_italic_font        Iosevka Term Bold Italic
disable_ligatures       cursor
font_features           +liga +calt +dlig
{{ if (eq .chezmoi.os "darwin") }}
font_size               14.0
{{ else }}
font_size               13.0
{{ end }}
include                 dracula.conf
window_padding_width    3
initial_window_width    132c
initial_window_height   43c
{{ if (eq .chezmoi.os "darwin") }}
shell                   /opt/homebrew/bin/fish --login --interactive
{{ else }}
shell                   /home/linuxbrew/.linuxbrew/bin/fish --login --interactive
{{ end }}
cursor_shape            block
cursor_blink_interval   0
shell_integration       disabled
macos_option_as_alt     yes

symbol_map U+23fb-U+23fe,U+2665,U+26a1,U+2b58,U+e000-U+e00a,U+e0a0-U+e0a2,U+e0a3,U+e0b0-U+e0b3,U+e0b4-U+e0c8,U+e0ca,U+e0cc-U+e0d4,U+e200-U+e2a9,U+e300-U+e3eb,U+e5fa-U+e631,U+e700-U+e7c5,U+ea60-U+ebeb,U+f000-U+f2e0,U+f300-U+f32d,U+f400-U+f4a8,U+f4a9,U+f500-U+fd46 Symbols Nerd Font Mono
