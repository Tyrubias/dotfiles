{
    "editor.bracketPairColorization.enabled": true,
    "editor.fontFamily": "Iosevka, Symbols Nerd Font Mono, Menlo, \"Apple Color Emoji\",  monospace",
    "editor.fontLigatures": "'ss01','ss05','dlig'",
    "editor.fontSize": 14,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": true,
    "editor.inlineSuggest.enabled": true,
    "editor.suggestSelection": "first",
    "errorLens.gutterIconsEnabled": true,
    "explorer.sortOrder": "type",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 2500,
    "git.mergeEditor": true,
    "latex-workshop.latex.autoBuild.run": "onSave",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.latexindent.path": "/Library/TeX/texbin/latexindent",
    "latex-workshop.view.pdf.viewer": "tab",
    "r.bracketedPaste": true,
    "r.rpath.mac": "/opt/homebrew/bin/R",
    "r.rterm.mac": "/Users/vsong/.local/bin/radian",
    "r.rterm.option": [
        "--no-save",
        "--no-restore",
        "--r-binary=/opt/homebrew/bin/R"
    ],
    "remote.SSH.lockfilesInTmp": true,
    "remote.SSH.remotePlatform": {
        "clear": "linux"
    },
    "rust-analyzer.checkOnSave.command": "clippy",
    "terminal.integrated.defaultProfile.osx": "zsh (interactive)",
    "terminal.integrated.fontFamily": "Iosevka Term, Symbols Nerd Font Mono, Menlo, \"Apple Color Emoji\",  monospace",
    "terminal.integrated.profiles.osx": {
        "zsh (interactive)": {
            "args": [
                "-i"
            ],
            "path": "/bin/zsh"
        }
    },
    "terminal.integrated.shellIntegration.enabled": true,
    "window.commandCenter": true,
    "window.newWindowDimensions": "inherit",
    "workbench.colorCustomizations": {
        "[Gruvbox Dark Hard]": {
            "editorInlayHint.background": "#3d3d3d48",
            "editorInlayHint.foreground": "#868686f0"
        }
    },
    "workbench.colorTheme": "Dracula",
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right"
}
