{
    "workbench.startupEditor": "newUntitledFile",
    "workbench.sideBar.location": "right",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "Rec Mono Semicasual",
    "terminal.integrated.fontFamily": "Rec Mono Semicasual",
    "files.autoSave": "afterDelay",
    "explorer.sortOrder": "type",
    "editor.fontLigatures": true,
    "window.newWindowDimensions": "inherit",
    "[typescript]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    },
    "oneDarkPro.bold": true,
    "oneDarkPro.vivid": true,
    "[json]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    },
    "workbench.colorTheme": "Dracula",
    "terminal.external.osxExec": "iTerm.app",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "python.venvPath": "/Users/vsong/mambaforge/envs",
    "terminal.integrated.inheritEnv": false,
    "[javascriptreact]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    },
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
        "ssh.clear.rice.edu": "linux",
        "clear": "linux",
        "local-pop": "linux"
    },
    "git.ignoreLegacyWarning": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "numso.prettier-standard-vscode"
    },
    "terminal.integrated.defaultProfile.linux": "/usr/bin/zsh",
    "python.languageServer": "Pylance",
    "editor.fontSize": 13,
    "terminal.integrated.fontSize": 13
}