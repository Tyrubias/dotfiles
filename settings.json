{
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "chat.agent.enabled": true,
  "chat.mcp.discovery.enabled": true,
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "'Iosevka', 'Symbols Nerd Font Mono', Menlo, 'Apple Color Emoji',  monospace",
  "editor.fontLigatures": "'liga', 'calt', 'dlig'",
  "editor.fontSize": 16,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": true,
  "editor.inlineSuggest.enabled": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "errorLens.gutterIconsEnabled": true,
  "explorer.sortOrder": "type",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2500,
  "git.allowForcePush": true,
  "git.autoStash": true,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.followTagsWhenSync": true,
  "git.mergeEditor": true,
  "git.rebaseWhenSync": true,
  "git.supportCancellation": true,
  "git.terminalGitEditor": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "github.gitProtocol": "ssh",
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.telemetry.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "latex-workshop.formatting.latexindent.path": "/Library/TeX/texbin/latexindent",
  "latex-workshop.latex.autoBuild.run": "onSave",
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "latex-workshop.view.pdf.viewer": "tab",
  "python.terminal.activateEnvironment": false,
  "r.bracketedPaste": true,
  "r.rpath.mac": "/opt/homebrew/bin/R",
  "r.rterm.mac": "/Users/vsong/.local/bin/radian",
  "r.rterm.option": [
    "--no-save",
    "--no-restore",
    "--r-binary=/opt/homebrew/bin/R"
  ],
  "remote.SSH.remotePlatform": {
    "victors-pop-os": "linux"
  },
  "remote.SSH.useLocalServer": false,
  "rust-analyzer.check.command": "clippy",
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.osx": "fish (interactive)",
  "terminal.integrated.fontFamily": "'Iosevka Term', 'Symbols Nerd Font Mono', Menlo, 'Apple Color Emoji',  monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.profiles.osx": {
    "fish (interactive)": {
      "args": ["-i"],
      "path": "/opt/homebrew/bin/fish"
    }
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "window.commandCenter": true,
  "window.newWindowDimensions": "inherit",
  "window.openFoldersInNewWindow": "on",
  "workbench.colorCustomizations": {
    "[Gruvbox Dark Hard]": {
      "editorInlayHint.background": "#3d3d3d48",
      "editorInlayHint.foreground": "#868686f0"
    }
  },
  "workbench.colorTheme": "Dracula Theme",
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.nextEditSuggestions.enabled": true
}
